{
    "executionRoleArn": "arn:aws:iam::146427984190:role/KreuzerServiceRoleForECSTaskExec",
    "taskRoleArn": "arn:aws:iam::146427984190:role/KreuzerServiceRoleForECSTaskSecApp",
    "family": "fargate-task-definition",
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "image": <IMAGE1_NAME>,
            "name": "sec-app",
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "essential": true,
            "logConfiguration": { 
              "logDriver": "awslogs",
              "options": { 
                 "awslogs-group" : "/ecs/fargate/sec-app",
                 "awslogs-region": "us-east-2",
                 "awslogs-stream-prefix": "ecs"
              }
           }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "revision": 8,
    "status": "ACTIVE"
}
